{
	"compileOnSave": false,
	"compilerOptions": {
		"jsx": "react",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "./",
		"paths": {
			"@brandModule/*": ["src/app/brand/*"],
			"@csAdminModule/*": ["src/app/cs-admin/*"],
			"@groupAdminModule/*": ["src/app/group-admin/*"],
			"@sharedModule/*": ["src/app/shared/*"],
			"@campaigns/*": ["src/app/group-admin/campaigns/*"],
			"crypto": ["./node_modules/crypto-browserify"],
			"stream": ["./node_modules/stream-browserify"]
		},
		"outDir": "./dist/out-tsc",
		"declaration": false,
		"module": "esnext",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"downlevelIteration": true,
		"target": "es2015",
		"typeRoots": ["node_modules/@types"],
		"lib": ["es2021", "dom"],
		"resolveJsonModule": true
	},
	"exclude": ["./global.before.ts"],
	"include": ["src", "playwright", "cypress"]
}
