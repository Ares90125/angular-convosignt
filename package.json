{
	"name": "convosight-web-client",
	"version": "1.0.0",
	"scripts": {
		"ng": "node --max_old_space_size=16000 node_modules/@angular/cli/bin/ng",
		"ng8": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng",
		"start": "npm run ng8 serve -- --ssl",
		"start-no-watch": "npm run ng serve -- --ssl --watch=false",
		"start-no-watch-ngrock": "npm run ng serve -- --disableHostCheck=true --ssl --watch=false",
		"build": "npm run ng build",
		"build:develop": "npm run ng build -- --configuration=develop",
		"build:pullrequest": "npm run ng build -- --configuration=pullrequest",
		"build:feature1": "npm run ng build -- --configuration=feature1",
		"build:feature2": "npm run ng build -- --configuration=feature2",
		"build:feature3": "npm run ng build -- --configuration=feature3",
		"build:feature4": "npm run ng build -- --configuration=feature4",
		"build:production": "npm run ng build -- --configuration=production",
		"build:staging": "npm run ng build -- --configuration=staging",
		"bundleAnalysis:production": "npm run ng build -- --configuration=production --stats-json && npm run analyze",
		"analyze": "webpack-bundle-analyzer dist/convosight-web-client/app/stats.json",
		"playwright:run": "playwright test",
		"lint": "npm run ng lint -- --fix",
		"cy:open": "cypress open --env coverage=true",
		"cy:run": "cypress run",
		"test:cypress-ui-regression": "concurrently \"npm run start-no-watch\" \"wait-on https-get://localhost:4200  && npm run cy:run -- --record --key a7094735-c588-4537-8862-1ca1261c5a2a --browser chrome --parallel --ci-build-id $BITBUCKET_BUILD_NUMBER --group ui-tests --spec 'cypress/tests/ui/**/*'\" --kill-others --success first",
		"test:cypress-ui-smoke": "concurrently \"npm run start-no-watch\" \"wait-on https-get://localhost:4200  && npm run cy:run -- --record --key a7094735-c588-4537-8862-1ca1261c5a2a --config-file cypress.smokeTest.json --env grepTags=@pullRequest --browser chrome --parallel --ci-build-id $BITBUCKET_BUILD_NUMBER --group ui-tests --spec 'cypress/tests/ui/**/*'\" --kill-others --success first",
		"test:cypress-api": "npm run cy:run -- --browser chrome --config-file cypress.api.json --env coverage=false",
		"clean-api": "node delete-cross-platform.js",
		"add-api:develop": "npm run clean-api && amplify codegen add --apiId 524cbwffvrannixl7xiyfsiht4",
		"add-api:feature1": "npm run clean-api && amplify codegen add --apiId vhav2am4pzdxbfqtbpdzmrf3ga",
		"add-api:feature2": "npm run clean-api && amplify codegen add --apiId hu6mr4ersngnbmbwxegck7z4xa",
		"add-api:production": "npm run clean-api && amplify codegen add --apiId jiygt4pksjgqdhmiltngm2wkju",
		"test-global": "ts-node-script --project tsconfig.server.json global.before",
		"delete-user": "ts-node-script --project tsconfig.server.json deleteTestUser.script",
		"test-global-before": "concurrently \"npm run start-no-watch\" \"wait-on https-get://localhost:4200 && npm run test-global\" --kill-others --success first",
		"test-playwright-1": "concurrently \"npm run start-no-watch\" \"wait-on https-get://localhost:4200 && npm run playwright:run -- --shard=1/3\" --kill-others --success first",
		"test-playwright-2": "concurrently \"npm run start-no-watch\" \"wait-on https-get://localhost:4200 && npm run playwright:run -- --shard=2/3\" --kill-others --success first",
		"test-playwright-3": "concurrently \"npm run start-no-watch\" \"wait-on https-get://localhost:4200 && npm run playwright:run -- --shard=3/3\" --kill-others --success first",
		"lighthouse": "ts-node-script --project tsconfig.server.json lighthouse.audit",
		"postinstall": "ngcc",
		"prepare": "husky install"
	},
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"Firefox ESR",
		"not dead",
		"not IE 9-11"
	],
	"lint-staged": {
		"**/*": [
			"prettier --write --ignore-unknown"
		]
	},
	"private": true,
